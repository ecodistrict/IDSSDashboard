{
    "name": "Numbers testing",
    "id": "numbersmodule",
    "description": "This module tests some different number inputs and returns a pie chart",
    "useKpis": [
        "kpi2"
    ],
    "inputs": [
        {
            "type": "input-group",
            "label": "Group 1",
            "inputs": [
                {
                    "label": "Integer between 50 and 100",
                    "type": "number",
                    "id": "int1",
                    "unit": "m",
                    "min": 50,
                    "max": 100
                },
                {
                    "label": "Positive float number with two digits",
                    "type": "number",
                    "min": 0,
                    "unit": "m",
                    "digits": 2,
                    "id": "int2"
                }
            ]
        },
        {
            "type": "input-group",
            "label": "Group 2",
            "inputs": [
                {
                    "label": "Just another number",
                    "type": "number",
                    "id": "int3",
                    "unit": "m",
                    "min": 50,
                    "max": 100
                },
                {
                    "label": "This should be checked",
                    "type": "checkbox",
                    "id": "check1"
                }
            ]
        }
    ],
    "outputs": [
        {
            "id": "output2",
            "type": "nvd3",
            "options": {
                "chart": {
                    "type": "pieChart",
                    "height": 300,
                    "width": 500,
                    "x": "key",
                    "y": "y",
                    "showLabels": true,
                    "transitionDuration": 500,
                    "labelThreshold": 0.01,
                    "legend": {
                        "margin": {
                            "top": 5,
                            "right": 35,
                            "bottom": 5,
                            "left": 0
                        }
                    }
                }
            },
            "data": [
                {

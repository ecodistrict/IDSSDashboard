{
	"name": "Text module",
	"id": "textmodule",
	"description": "This module takes a text string and returns a bar chart of letter index",
	"useKpis": ["kpi1"],
	"inputs": [{
		"type": "input-group",
		"label": "Input group",
		"inputs": [
			{
				"label": "Text to analyse",
				"type": "text",
				"id": "text1"
			},
			{
				"label": "Reverse",
				"type": "checkbox",
				"id": "check1"
			}
		]
	}],
	"outputs": [{
		"type": "barchart",
		"labels": {
			"y": "Point"
		},
		"data": [
			{
				"letter": "A",
				"value": 1
			},
			{
				"letter": "B",
				"value": 2
			},
			{
				"letter": "C",
				"value": 3
			}
		]
	}]
}